
@{
    ViewBag.Calc = 20 / 5;
}

@model ProjectsViewModel
@*<h1>@ViewBag.Calc</h1>*@
<h1>The Water Project</h1>

@foreach (Project p in Model.Projects)
{
    <h3>@p.ProjectName</h3>
    <ul>
        <li>Type: @p.ProjectType</li>
        <li>Program: @p.ProjectRegionalProgram</li>
        <li>Impact: @p.ProjectImpact individuals served</li>
        <li>Project Phase: @p.ProjectPhase</li>
        <li>Functionality Status: @p.ProjectFunctionalityStatus</li>
    </ul>
}

<a asp-action="Index" asp-route-pageNum="1">1</a>
<a asp-action="Index" asp-route-pageNum="2">2</a>
<a asp-action="Index" asp-route-pageNum="3">3</a>

<div page-blah="@Model.PageInfo" page-action="Index"></div>